{
  "uid": "sabine-overview",
  "title": "Sabine Agent Overview",
  "tags": ["sabine", "dream-team", "queue"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "Queue Depth",
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 6,
        "h": 8
      },
      "targets": [
        {
          "expr": "dream_team_queue_depth{status=\"queued\"}",
          "legendFormat": "Queued",
          "refId": "A"
        },
        {
          "expr": "dream_team_queue_depth{status=\"in_progress\"}",
          "legendFormat": "In Progress",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10,
            "showPoints": "never"
          },
          "unit": "short"
        }
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      }
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Blocked Tasks",
      "gridPos": {
        "x": 6,
        "y": 0,
        "w": 6,
        "h": 8
      },
      "targets": [
        {
          "expr": "dream_team_blocked_tasks",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "green"
              },
              {
                "value": 1,
                "color": "red"
              }
            ]
          },
          "unit": "short"
        }
      },
      "options": {
        "textMode": "value_and_name",
        "colorMode": "background",
        "graphMode": "none",
        "orientation": "auto"
      }
    },
    {
      "id": 3,
      "type": "stat",
      "title": "Stuck Tasks",
      "gridPos": {
        "x": 12,
        "y": 0,
        "w": 6,
        "h": 8
      },
      "targets": [
        {
          "expr": "dream_team_stuck_tasks",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "green"
              },
              {
                "value": 1,
                "color": "red"
              }
            ]
          },
          "unit": "short"
        }
      },
      "options": {
        "textMode": "value_and_name",
        "colorMode": "background",
        "graphMode": "none",
        "orientation": "auto"
      }
    },
    {
      "id": 4,
      "type": "bargauge",
      "title": "Stale Tasks",
      "gridPos": {
        "x": 18,
        "y": 0,
        "w": 6,
        "h": 8
      },
      "targets": [
        {
          "expr": "dream_team_stale_tasks{threshold=\"1h\"}",
          "legendFormat": "1h",
          "refId": "A"
        },
        {
          "expr": "dream_team_stale_tasks{threshold=\"24h\"}",
          "legendFormat": "24h",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "green"
              },
              {
                "value": 1,
                "color": "yellow"
              },
              {
                "value": 5,
                "color": "red"
              }
            ]
          },
          "unit": "short"
        }
      },
      "options": {
        "displayMode": "gradient",
        "orientation": "horizontal",
        "showUnfilled": true
      }
    },
    {
      "id": 5,
      "type": "gauge",
      "title": "Success Rate (1h)",
      "gridPos": {
        "x": 0,
        "y": 8,
        "w": 6,
        "h": 8
      },
      "targets": [
        {
          "expr": "dream_team_success_rate_1h",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "red"
              },
              {
                "value": 70,
                "color": "yellow"
              },
              {
                "value": 90,
                "color": "green"
              }
            ]
          },
          "unit": "percent",
          "min": 0,
          "max": 100
        }
      },
      "options": {
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      }
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "Completed vs Failed",
      "gridPos": {
        "x": 6,
        "y": 8,
        "w": 6,
        "h": 8
      },
      "targets": [
        {
          "expr": "dream_team_completed_1h",
          "legendFormat": "Completed",
          "refId": "A"
        },
        {
          "expr": "dream_team_failed_1h",
          "legendFormat": "Failed",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10,
            "showPoints": "never"
          },
          "unit": "short"
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Completed"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "green",
                  "mode": "fixed"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Failed"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "red",
                  "mode": "fixed"
                }
              }
            ]
          }
        ]
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      }
    },
    {
      "id": 7,
      "type": "timeseries",
      "title": "Task Duration",
      "gridPos": {
        "x": 12,
        "y": 8,
        "w": 6,
        "h": 8
      },
      "targets": [
        {
          "expr": "dream_team_task_duration_ms{quantile=\"avg\"}",
          "legendFormat": "Average",
          "refId": "A"
        },
        {
          "expr": "dream_team_task_duration_ms{quantile=\"p95\"}",
          "legendFormat": "P95",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10,
            "showPoints": "never"
          },
          "unit": "ms"
        }
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      }
    },
    {
      "id": 8,
      "type": "stat",
      "title": "Pending Retries",
      "gridPos": {
        "x": 18,
        "y": 8,
        "w": 6,
        "h": 8
      },
      "targets": [
        {
          "expr": "dream_team_pending_retries",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "green"
              },
              {
                "value": 1,
                "color": "yellow"
              },
              {
                "value": 5,
                "color": "red"
              }
            ]
          },
          "unit": "short"
        }
      },
      "options": {
        "textMode": "value_and_name",
        "colorMode": "background",
        "graphMode": "none",
        "orientation": "auto"
      }
    }
  ]
}
